    connect_str = 'DefaultEndpointsProtocol=https;AccountName=otteropotamus;AccountKey=jdcun1cDS7Dva2pzzl8AVm2ko4IJLzUj1dWLGhTeygvCa/PRz0l68aWORHeA7giJMiICi1JofFeEZV8kxeaiIQ==;EndpointSuffix=core.windows.net'

